%h2 Sign up
= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  = devise_error_messages!
  %p
    = f.text_field :first_name, :placeholder => "First Name", :class => "title"
  %p
    = f.text_field :last_name, :placeholder => "Last name", :class => "title"
  %p
    = f.text_field :nickname, :placeholder => "Nickname", :class=> "title"
  %p
    = f.text_field :email, :placeholder => "Email", :class => "title"
  %p
    = f.password_field :password, :placeholder => "Password", :class => "title"
  %p
    = f.password_field :password_confirmation, :placeholder => "Confirm Password", :class => 'title'
  %p= f.submit "Create Account", :class=>"cupid-green"
= render :partial => "devise/shared/links"
